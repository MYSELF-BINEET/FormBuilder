#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bineetpradhan/Documents/PROGRAMS/FormBuilder/node_modules/.pnpm/tailwindcss@3.3.3_ts-node@10.9.1_@swc+core@1.3.94_@types+node@20.8.7_typescript@5.2.2_/node_modules/tailwindcss/lib/node_modules:/Users/bineetpradhan/Documents/PROGRAMS/FormBuilder/node_modules/.pnpm/tailwindcss@3.3.3_ts-node@10.9.1_@swc+core@1.3.94_@types+node@20.8.7_typescript@5.2.2_/node_modules/tailwindcss/node_modules:/Users/bineetpradhan/Documents/PROGRAMS/FormBuilder/node_modules/.pnpm/tailwindcss@3.3.3_ts-node@10.9.1_@swc+core@1.3.94_@types+node@20.8.7_typescript@5.2.2_/node_modules:/Users/bineetpradhan/Documents/PROGRAMS/FormBuilder/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bineetpradhan/Documents/PROGRAMS/FormBuilder/node_modules/.pnpm/tailwindcss@3.3.3_ts-node@10.9.1_@swc+core@1.3.94_@types+node@20.8.7_typescript@5.2.2_/node_modules/tailwindcss/lib/node_modules:/Users/bineetpradhan/Documents/PROGRAMS/FormBuilder/node_modules/.pnpm/tailwindcss@3.3.3_ts-node@10.9.1_@swc+core@1.3.94_@types+node@20.8.7_typescript@5.2.2_/node_modules/tailwindcss/node_modules:/Users/bineetpradhan/Documents/PROGRAMS/FormBuilder/node_modules/.pnpm/tailwindcss@3.3.3_ts-node@10.9.1_@swc+core@1.3.94_@types+node@20.8.7_typescript@5.2.2_/node_modules:/Users/bineetpradhan/Documents/PROGRAMS/FormBuilder/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
